{
    "builds": [{
        "src": "index.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" , "wsgiApplicationName": "app"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/spots_detector/wsgi.py"
        }
    ],
    "outputDirectory": "build"
}