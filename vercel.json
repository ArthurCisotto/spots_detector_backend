{
    "builds": [{
        "src": "spots_detector/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/spots_detector/wsgi.py"
        }
    ]
}